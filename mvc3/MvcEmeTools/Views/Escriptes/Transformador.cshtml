@model Dominio.Entidades.Escripte
@{
    ViewBag.Title = "Transformador";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.Partial("menu_edit_sequencer", Model.IdSha1)
@section js{
    <script>
        var idSha1 = '@Model.IdSha1';
    </script>
    <script src="@Url.Content("~/Scripts/EmeTools/jsed/jsed.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/EmeTools/emetools.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/EmeTools/roboXixi.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/EmeTools/xixizero.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/EmeTools/transformarXxx.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/EmeTools/comandos.js")" type="text/javascript"> </script>
    <script src="@Url.Content("~/Scripts/Escriptes/Transformador.js")" type="text/javascript"> </script>
}
<link href="@Url.Content("~/Content/Escriptes/Sequencer.css")" rel="stylesheet" type="text/css" />
<h3>Transformador: @Model.Nome</h3>@Model.Descricao
<div style="height: 550px; width: 980px; ">
    <textarea id="textAreaDados" style="height: 100%; width: 100%;" wrap="off"></textarea>
</div>
<div style="height: 50px; width: 990px; text-align: right; margin-top: 10px;">
    Ctrl + ENTER
    <input type="button" id="buttonTransformar" value="Transformar"/>
</div>